{
  "compilerOptions": {
    "target": "es5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": false,
    "inlineSourceMap": true,
    "baseUrl": ".",
    "outDir": "out",
    "lib": [
      "es2015"
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "out"
  ]
}